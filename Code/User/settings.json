{
  "[yaml]": {
    "diffEditor.ignoreTrimWhitespace": true,
  },
  "chat.agent.enabled": false,
  "chat.commandCenter.enabled": false,
  "chat.fontSize": 14,
  "color-picker-universal.avoidDuplicate": true,
  "color-picker-universal.formatsFrom": [
    "device-cmyk",
    "hex",
    "hex0x",
    "hsl",
    "hwb",
    "rgb",
    "lab",
    "lch",
    "oklab",
    "oklch",
  ],
  "color-picker-universal.ignoreVariableName": false,
  "color-picker-universal.languages": ["*", "!css", "!less", "!scss", "!sass"],
  "css.lint.emptyRules": "ignore",
  "css.lint.unknownAtRules": "ignore",
  "editor.codeActionsOnSave": {
    "source.fixAll.biome": "explicit",
    "source.organizeImports.biome": "explicit",
  },
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.fontWeight": "500",
  "editor.formatOnSave": true,
  "editor.lineHeight": 2,
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.scale": 2,
  "editor.stickyScroll.enabled": false,
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "explorer.fileNesting.enabled": true,
  "explorer.sortOrder": "type",
  "files.associations": {
    "*.kdl": "plaintext",
  },
  "files.autoSave": "afterDelay",
  "git.autofetch": true,
  "git.openRepositoryInParentFolders": "never",
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.preferences.useAliasesForRenames": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "python.createEnvironment.trigger": "off",
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "svelte.enable-ts-plugin": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "terminal.integrated.fontLigatures.enabled": true,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontWeight": "600",
  "terminal.integrated.letterSpacing": 1.4,
  "terminal.integrated.lineHeight": 1.4,
  "terminal.integrated.stickyScroll.enabled": false,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.useAliasesForRenames": false,
  "window.commandCenter": false,
  "window.customTitleBarVisibility": "auto",
  "window.menuBarVisibility": "hidden",
  "window.titleBarStyle": "native",
  "window.zoomLevel": 1.4,
  "workbench.activityBar.location": "hidden",
  "workbench.colorTheme": "Gruvbox Dark Medium",
  "workbench.editor.showTabs": "single",
  "workbench.editor.tabSizing": "fixed",
  "workbench.editor.tabSizingFixedMinWidth": 200,
  "workbench.iconTheme": "catppuccin-macchiato",
  "workbench.layoutControl.enabled": false,
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 16,
  "terminal.integrated.initialHint": false,
}
